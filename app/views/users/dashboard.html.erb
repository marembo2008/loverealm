<div class="ct profile">
	<div class="acc-info-pn">
		<div class="acc-info">
			<%= image_tag(image_path('account-pic.jpg'), class: "pp")%>
			<a href="#" class="ecp">Edit Cover Photo</a>
			<div class="clear"></div>
		</div>
		<div class="clear"></div>
		<br />
		<br />
		<br />
		<div class="g-info">
			<span>Yaw Ansong</span>
		</div>
		<div class="bio">
			<span>CEO of Loverealm.org network</span>
			<br/>
			<span>Registered on 8th January, 2014</span>
			<br/>
			<span class="love-score">Love Score: 1000 <a href="#" class="more-info">?</a></span>
			<br/>
			<span>Ranking: <span class="ranking">8th most loved person</span></span>
			<br/>
			<br/>
			<a href="#" class="inc-ranking">Increase your ranking</a>
		</div>
		<br/>
	</div>
	<div class="dh">
		<div class="dheader">
			<a href="#" class="active-option" data-href="myaccount">My Content</a>
			<a href="#" data-href="people-i-love">People I love</a>
			<a href="#">My stored posts</a>
			<a href="#" data-href="inbox">Inbox</a>
			<a href="#" data-href="notifications">Notifications</a>
			<a href="#" data-href="preferences">Preferences</a>
		</div>
		<div class="dcontent">
			<div class="dcontent-options myaccount">
				<%= render "users/dashboard/myaccount"%>
			</div>
			<div class="dcontent-options people-i-love">
				<%= render "users/dashboard/love"%>
			</div>
			<div class="dcontent-options inbox">
				<%= render "users/dashboard/inbox"%>
			</div>
			<div class="dcontent-options notifications">
				<%= render "users/dashboard/notifications"%>
			</div>
			<div class="dcontent-options preferences">
				<%= render "users/dashboard/preferences"%>
			</div>
		</div>
	</div>
	<div class="clear"></div>
</div>
